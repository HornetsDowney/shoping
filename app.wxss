@import "./styles/iconfont.wxss";

/* 在微信小程序中不支持通配符，例如“*” */
page,view,text,swiper,swiper-itme,image,navigator{
    padding: 0%;
    margin: 0%;
    box-sizing: border-box;
}

/* 
    主题颜色通过变量来实现
        1.less中存在变量这个知识
        2.原生的css和wxss都支持变量
*/
page{
    /* 定义主题颜色 */
    --themeColor:#eb4450;
}